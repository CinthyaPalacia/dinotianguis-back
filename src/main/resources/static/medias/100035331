<script type="61c5af9d698247151ea2975d-text/javascript">
	function addToCartDatalayer(sku,name,catego,marc,precio) {
		dataLayer.push({
			'event': 'addToCart',
			'ecommerce': {
				'add': {
					'products': [{
						'name': name,
						'id': sku,
						'category': catego,
						'brand': marc,
						'price': precio,
						'quantity': 1
					}]
				}
			}
		});
	}
</script>
